{
    "version": 2,
    "buildCommand": "next build",
    "devCommand": "next dev",
    "installCommand": "npm install",
    "framework": "nextjs",
    "builds": [
        {
            "src": "app/api/python/route.py",
            "use": "@vercel/python",
            "config": {
                "maxDuration": 60
            }
        }
    ],
    "routes": [
        {
            "src": "/api/extract",
            "dest": "app/api/python/route.py"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
} 